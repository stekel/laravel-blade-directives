{
    "name": "stekel/laravel-blade-directives",
    "type": "library",
    "description": "A personal collection of Laravel blade directives.",
    "license": "MIT",
    "homepage": "https://github.com/stekel/laravel-blade-directives",
    "keywords": [
        "Laravel",
        "Blade",
        "Directives"
    ],
    "require": {
        "illuminate/support": "~5",
        "php" : "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "stekel\\LaravelBladeDirectives\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stekel\\LaravelBladeDirectives\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "stekel\\LaravelBladeDirectives\\LaravelBladeDirectivesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
